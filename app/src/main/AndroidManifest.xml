<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hsue.sue.level" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OutMapActivity"
            android:label="IndoorMap"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".InMapActivity"
            android:label="IndoorMap"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".FloorActivity"
            android:label="@string/title_activity_floor"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".IndoorActivity"
            android:label="@string/title_activity_indoor"
            android:screenOrientation="landscape" >
        </activity>
    </application>

</manifest>
